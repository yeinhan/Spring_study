<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- 생성자 -->
	<bean id="lee" class="com.test02.Emp">
		<constructor-arg name="name" value="이순신"></constructor-arg>
		<constructor-arg name="num" value="200000"></constructor-arg>	
	</bean>
	
<!-- setter -->
	<bean  id="hong" class="com.test02.Emp">
		<property name="name" value="홍길동"></property>
		<property name="num" value="250000"></property>
		</bean>
</beans>
